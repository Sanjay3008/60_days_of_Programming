arr1 =[1, 2, 3, 4, 5]
arr2 = [1,2,3]

i = 0
j = 0

while(i<len(arr1) and j<len(arr2)):

    if(arr1[i]<arr2[j]):
        print(arr1[i])
        i=i+1
    elif(arr1[i]>arr2[j]):
        print(arr2[j])
        j=j+1
    else:
        print(arr1[i])
        i=i+1
        j=j+1

while i < len(arr1):
    print(arr1[i])
    i += 1

while j < len(arr2):
    print(arr2[j])
    j += 1
